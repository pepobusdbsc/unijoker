  <script>

    $.ajaxPrefilter(function(options, originalOptions, jqXHR){
    if (options.type.toLowerCase() === "post") {
      jqXHR.setRequestHeader('X-CSRFToken', $('meta[name="csrf-token"]').attr('content'));
    }
  });

function sliderChangeWithdraw(){
    //$('#slide-percentage')[0].innerText = $('#myRange')[0].value + "%";
    percentSwitchWithdraw($('#myRangeWithdraw')[0].value);
}


function percentSwitchWithdraw(percent){
    $('#myRangeWithdraw')[0].value = percent;
    $('#slide-percentage-withdraw')[0].innerText = percent + "%"; 

    if (percent == 100){
        $("#managelock_withdraw_input").val($("#managelock_withdraw_available_display span").text());
    }
    else{
        var amount = parseFloat($("#managelock_withdraw_available_display span").text());
        var dec_count = decimalCount(amount);
        if (dec_count < 2){ dec_count = 2;}

        var amount = percent * amount /100;
        // TODO: can use the actual token decimals instead of hardcoding to 6
        if (decimalCount(amount) > dec_count){
            amount = amount.toFixed(dec_count);
        }
        $("#managelock_withdraw_input").val(amount);

    }
}



function sliderChangeIncrement(){
    //$('#slide-percentage')[0].innerText = $('#myRange')[0].value + "%";
    percentSwitchIncrement($('#myRangeIncrement')[0].value);
}


function percentSwitchIncrement(percent){
    $('#myRangeIncrement')[0].value = percent;
    $('#slide-percentage-increment')[0].innerText = percent + "%"; 

    if (percent == 100){
        $("#managelock_increment_input").val($("#managelock_increment_available_display span").text());
    }
    else{
        var amount = parseFloat($("#managelock_increment_available_display span").text());
        var dec_count = decimalCount(amount);
        if (dec_count < 2){ dec_count = 2;}

        var amount = percent * amount /100;
        // TODO: can use the actual token decimals instead of hardcoding to 6
        if (decimalCount(amount) > dec_count){
            amount = amount.toFixed(dec_count);
        }
        $("#managelock_increment_input").val(amount);

    }
}

function sliderChangeMonthExtend(){
    //$('#slide-percentage-month')[0].innerText = $('#myRangeMonth')[0].value + "%";
    monthDivExtend($('#myRangeMonthExtend')[0].value);
}


function monthDivExtend(percent){
    $('#myRangeMonthExtend')[0].value = percent;
    $('#slide-percentage-month-extend')[0].innerText = percent + "%"; 

    var new_date = new Date();
    new_date.setMonth( new_date.getMonth() + percent*60/100);
    document.querySelector("#managelock_extend_input")._flatpickr.setDate(new_date, true);
 

}



function sliderChange(){
    //$('#slide-percentage')[0].innerText = $('#myRange')[0].value + "%";
    percentSwitch($('#myRange')[0].value);
}

function sliderChangeMonth(){
    //$('#slide-percentage-month')[0].innerText = $('#myRangeMonth')[0].value + "%";
    monthDiv($('#myRangeMonth')[0].value);
}

function sliderChangeMonthVesting(){
    //$('#slide-percentage-month')[0].innerText = $('#myRangeMonth')[0].value + "%";
    monthDivVesting($('#myRangeMonthVesting')[0].value);
}


function sliderChangeMonthExtendVesting(){
    //$('#slide-percentage-month')[0].innerText = $('#myRangeMonth')[0].value + "%";
    monthDivExtendVesting($('#myRangeMonthExtendVesting')[0].value);
}



function percentSwitch(percent){
    $('#myRange')[0].value = percent;
    $('#slide-percentage')[0].innerText = percent + "%"; 

    if (percent == 100){
        $("#newlock_amount").val($("#newlock_lptoken_balance_info").text());
    }
    else{
        var amount = parseFloat($("#newlock_lptoken_balance_info").text());
        var dec_count = decimalCount(amount);
        if (dec_count < 2){ dec_count = 2;}

        var amount = percent * amount /100;
        // TODO: can use the actual token decimals instead of hardcoding to 6
        if (decimalCount(amount) > dec_count){
            amount = amount.toFixed(dec_count);
        }
        $("#newlock_amount").val(amount);

    }
}
function monthDiv(percent){
    $('#myRangeMonth')[0].value = percent;
    $('#slide-percentage-month')[0].innerText = percent + "%"; 

    var new_date = new Date();
    new_date.setMonth( new_date.getMonth() + percent*60/100);
    document.querySelector("#newlock_unlocktime")._flatpickr.setDate(new_date, true);
 

}

function monthDivVesting(percent){
    $('#myRangeMonthVesting')[0].value = percent;
    $('#slide-percentage-month-vesting')[0].innerText = percent + "%"; 

    var new_date = new Date();
    new_date.setMonth( new_date.getMonth() + percent*60/100);
    document.querySelector("#newlock_unlocktime_vesting")._flatpickr.setDate(new_date, true);
 

}

function monthDivExtendVesting(percent){
    $('#myRangeMonthExtendVesting')[0].value = percent;
    $('#slide-percentage-month-extend-vesting')[0].innerText = percent + "%"; 

    var new_date = new Date();
    new_date.setMonth( new_date.getMonth() + percent*60/100);
    document.querySelector("#managelock_unlocktime_extend_vesting")._flatpickr.setDate(new_date, true);
 

}

const decimalCount = num => {
    // Convert to String
    const numStr = String(num);
    // String Contains Decimal
    if (numStr.includes('.')) {
       return numStr.split('.')[1].length;
    };
    // String Does Not Contain Decimal
    return 0;
 }

